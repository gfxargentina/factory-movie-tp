{
  "name": "factory-movie-tp",
  "version": "1.0.0",
  "description": "Trabajo Practico Final - Skill Factory NodeJS by Avalith",
  "main": "index.js",
  "scripts": {
    "start": "npm run db:create && npm run db:migrate && node src/index.js",
    "dev": "nodemon src/index.js",
    "test-coverage": "nyc --reporter=html --reporter=text mocha src/test/app.test.js --exit ",
    "test": "mocha src/test/app.test.js --exit ",
    "db:create": "npx sequelize-cli db:create",
    "db:migrate": "npx sequelize-cli db:migrate",
    "db:seed": "npx sequelize-cli db:seed:all",
    "db:undoall": "npx sequelize-cli db:migrate:undo:all"
  },
  "author": "Luis Chilo",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "chai": "^4.3.6",
    "chai-http": "^4.3.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "helmet": "^6.0.0",
    "jsonwebtoken": "^8.5.1",
    "mocha": "^10.1.0",
    "morgan": "^1.10.0",
    "node-fetch": "^3.2.10",
    "nodemailer": "^6.8.0",
    "nodemon": "^2.0.20",
    "pg": "^8.8.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.25.3",
    "supertest": "^6.3.1",
    "swagger-jsdoc": "^6.2.5",
    "swagger-ui-express": "^4.6.0"
  },
  "devDependencies": {
    "nyc": "^15.1.0",
    "sequelize-cli": "^6.5.2"
  }
